{
  "name": "gerardo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "vue-cli-service test:unit",
    "test-hook": "vue-cli-service test:unit --changedSince=@{push}",
    "lint": "vue-cli-service lint",
    "prepare": "cd .. && husky install gerardo/.husky"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "firebase": "^9.6.6",
    "moment": "^2.29.1",
    "node-fetch": "^3.2.0",
    "vue": "^2.6.11",
    "vue-router": "^3.5.3",
    "vue-test-utils": "^1.0.0-beta.11",
    "vuefire": "^2.2.5",
    "vuex": "^3.6.2",
    "vuexfire": "^3.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/test-utils": "^1.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "jest-date-mock": "^1.0.8",
    "prepush-if-changed": "^1.0.8",
    "vue-template-compiler": "^2.6.11",
    "jest": "^27.1.0",
    "@vue/vue2-jest": "^27.0.0-alpha.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
